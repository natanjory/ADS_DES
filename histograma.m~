A = importdata('Gerado_Arrival.txt');
D = importdata('Gerado_Departure.txt');

subplot(2,1,1);
[h,centers]=hist(A,500);
h=h./length(h);
bar(centers,h)
title(' Distribuição entre requisições');

subplot(2,1,2);
[h2,centers2]=hist(D,300);
h2=h2./length(h2);
bar(centers2,h2)
title(' Distribuição entre requisições');
%title(' Distribuição entre serviços');
